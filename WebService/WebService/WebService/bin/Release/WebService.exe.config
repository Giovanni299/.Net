          </binding>
          <binding name="NetTcpBinding_IPublisherService" />
          <binding name="NetTcpBinding_IBusinessSubsProcess">
            <security mode="None" />
          </binding>
        </netTcpBinding>
      </bindings>
       
    </system.serviceModel>
